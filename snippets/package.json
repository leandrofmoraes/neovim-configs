{
	"name": "my-custom-snippets",
	"engines": {
		"vscode": "^1.11.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": ["sql"],
				"path": "./sql.json"
			},
			{
				"language": ["lua"],
				"path": "./lua.json"
			},
			{
				"language": ["markdown"],
				"path": "./markdown.json"
			},
			{
				"language": [
					"javascript",
					"javascriptreact",
					"vue",
					"svelte",
					"astro",
					"javascript.jsx",
					"typescript",
					"typescriptreact",
					"typescript.tsx"
				],
				"path": "./javascript.json"
			},
			{
				"language": [
					"javascriptreact",
					"javascript.jsx",
					"typescriptreact",
					"typescript.tsx"
				],
				"path": "./javascriptreact.json"
			},
			{
				"language": ["tex"],
				"path": "./tex.json"
			}
		]
	}
}
